@model ConstructIT.DAL.Models.Slika

@{
    ViewBag.Title = "Brisanje slike";
}

<h2>Brisanje</h2>

<h3 style="padding-left: 3%">Da li ste sigurni da želite da obrišete?</h3>
<div>
    <h4>Slika</h4>
    <hr />


    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Obriši" class="btn btn-default" /> |
            @Html.ActionLink("Odustani", "Details", new { naziv = Model.SlikaNaziv })
        </div>
    }
</div>
