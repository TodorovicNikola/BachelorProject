@model IEnumerable<ConstructIT.DAL.Models.TipMasine>

@{
    ViewBag.Title = "Tipovi masina";
}

<h2>Tipovi mašina</h2>

<p>
    @Html.ActionLink("Dodaj novi", "Create")
</p>

<hr />

<table class="table table-striped table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TipMasineNaziv)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TipMasineNaziv)
        </td>
        <td>
            @Html.ActionLink("Izmeni", "Edit", new { id=item.TipMasineID }) |
            @Html.ActionLink("Obriši", "Delete", new { id=item.TipMasineID })
        </td>
    </tr>
}

</table>
